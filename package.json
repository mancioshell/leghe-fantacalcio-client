{
  "name": "fantasy-football-wrapper",
  "version": "1.2.1",
  "description": "A client to interact with fantacalcio.it REST Api",
  "author": "Alessandro Mancini <dott.alessandro.mancini@gmail.com>",
  "license": "ISC",
  "main": "dist/index.js",
  "types": "dist/index.d.ts",
  "scripts": {
    "start": "ts-node ./examples/sample.ts",
    "build": "tsc",
    "prepublish": "tsc",
    "commit": "git-cz",
    "release": "env HUSKY_SKIP_HOOKS=1 standard-version --commit-all",
    "platform": "run-script-os",
    "platform:win32": "echo 'Skipping this hook in Windows platform'",
    "platform:nix": "exec < /dev/tty && git cz --hook || true",
    "prepare": "husky install"
  },
  "dependencies": {
    "set-cookie-parser": "^2.5.1",
    "unirest": "^0.6.0"
  },
  "devDependencies": {
    "@commitlint/cli": "^17.0.3",
    "@commitlint/config-conventional": "^17.0.3",
    "@types/node": "^18.6.3",
    "commitizen": "^4.2.5",
    "cross-env": "^7.0.3",
    "husky": "^8.0.1",
    "run-script-os": "^1.1.6",
    "standard-version": "^9.5.0",
    "ts-node": "^10.9.1",
    "typescript": "^4.7.4"
  },
  "config": {
    "commitizen": {
      "path": "./node_modules/cz-conventional-changelog"
    }
  },
  "commitlint": {
    "extends": [
      "@commitlint/config-conventional"
    ]
  }
}
